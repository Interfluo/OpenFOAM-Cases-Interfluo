FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      nut;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];

internalField   uniform 7.96;

boundaryField
{
    topZ
    {
        type            calculated;
		value			uniform 7.96;
    }

    bottomZ
    {
        type            calculated;
		value			uniform 7.96;
    }

    topX
    {
        type            nutkWallFunction;
        value           uniform 7.96;
    }

    bottomX
    {
        type            nutkWallFunction;
        value           uniform 7.96;
    }
	
	topY
    {
        type            nutkWallFunction;
        value           uniform 7.96;
    }

    bottomY
    {
        type            nutkWallFunction;
        value           uniform 7.96;
    }
	
	f150
    {
        type            nutkWallFunction;
        value           uniform 7.96;
    }

}

// ************************************************************************* //
