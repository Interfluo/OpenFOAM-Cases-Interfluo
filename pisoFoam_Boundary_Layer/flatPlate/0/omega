FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      omega;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 -1 0 0 0 0];

internalField   uniform 6.12e-2;

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           $internalField;
    }
	
    outlet
    {
        type            zeroGradient;
    }
	
    top
    {
        type            omegaWallFunction;
        value           $internalField;
    }
	
    frontAndBack
    {
        type            empty;
    }

	plate
    {
        type            omegaWallFunction;
        value           $internalField;
    }
}


// ************************************************************************* //
